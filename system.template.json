{
  "name": "zweihander",
  "title": "Zweihänder Grim & Perilous RPG",
  "description": "An implementation of the Zweihänder system for FoundryVTT.",
  "version": "{{version}}",
  "minimumCoreVersion": "0.8.4",
  "compatibleCoreVersion": "9",
  "templateVersion": 2,
  "authors": [
    {
      "name": "Re4XN",
      "discord": "Re4XN#8341"
    },
    {
      "name": "kxfin",
      "discord": "kxfin#0458"
    }
  ],
  "esmodules": ["zweihander.js"],
  "styles": ["zweihander.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "gridDistance": 1,
  "gridUnits": "yd",
  "socket": true,
  "url": "https://github.com/fh-fvtt/zweihander",
  "manifest": "{{manifest}}",
  "download": "https://github.com/fh-fvtt/zweihander/releases/download/v{{version}}/system.zip",
  "license": "LICENCE.txt",
  "primaryTokenAttribute": "stats.secondaryAttributes.damageCurrent",
  "secondaryTokenAttribute": "stats.secondaryAttributes.perilCurrent",
  "dependencies": [
    {
