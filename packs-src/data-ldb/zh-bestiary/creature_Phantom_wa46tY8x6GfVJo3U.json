      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures make a successful attack bare-handed, they provoke Fear, but do no Damage. They also force a foe to Resist with a Resolve Test or be aged by one year. For every year aged, the foe permanently reduces Brawn by 1%."
          }
        }
      },
      "type": "trait"
    },
