    {
      "_id": "6XQ342g3Sbzwu1rb",
      "effects": [],
      "img": "systems/zweihander/assets/icons/wingfoot.svg",
      "name": "Fast on Their Feet",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures reduce all Movement Actions by 1 AP (to a minimum of 1 AP). They can also Dodge both melee and ranged weapons."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "8YUSzoy7SjxI4RHW",
      "data": {
        "description": {
          "@en": "Although you may not harness the Magicks granted by a deity, you are no less blessed by your god. Living a hardscrabble life upon the road, the religious habit you wear and symbol you carry are blessed, safeguarding you against wrongdoers."
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "Whenever you wear the robes of your office and bear a holy symbol around your neck, add +1 to both your Damage and Peril Thresholds. Furthermore, you may add the Defensive Quality to any weapon you wield."
          }
        }
      },
      "effects": [],
      "img": "systems/zweihander/assets/icons/vitruvian-man.svg",
      "name": "Holy Raiment",
      "type": "trait"
    },
    {
      "_id": "hpw5QTS219IUnPmS",
      "effects": [],
      "img": "systems/zweihander/assets/icons/lightning-electron.svg",
      "name": "Physical Instability",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "Every minute these creatures remain in the Material Realm in physical form, roll 3D6 Chaos Dice at the end of their Turn. If all three dice show face ‘6’, the creature is banished from the Material Realm, until the new moon."
          }
        }
      },
      "type": "trait"
    },
    {
