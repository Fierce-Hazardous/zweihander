    {
      "_id": "3id21PS6900HTMM2",
      "effects": [],
      "img": "systems/zweihander/assets/icons/floor-hatch.svg",
      "name": "Call of the Abyss",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When this creature suffers Damage and is unable to deal Damage by the end of its next Turn, roll 3D6. If all three dice show face ‘6’, the creature is banished back to the Abyss, until summoned once again."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "BezEGaTh6AvpUX6y",
      "effects": [],
      "img": "systems/zweihander/assets/icons/rune-stone.svg",
      "name": "Ceremonial Runes",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures are encountered, roll 1D6 Chaos Die. If the result is face ‘1’, ‘2’ or ‘3’, they have three Apprentice Runes inscribed upon their staff. If the result is face ‘4’ or ‘5’, they have two Journeyman Runes inscribed upon their staff. If the result is face ‘6’, they have one Master Rune inscribed upon their staff."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "6XQ342g3Sbzwu1rb",
      "effects": [],
      "img": "systems/zweihander/assets/icons/wingfoot.svg",
      "name": "Fast on Their Feet",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures reduce all Movement Actions by 1 AP (to a minimum of 1 AP). They can also Dodge both melee and ranged weapons."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "ipFEoJjjBXNbONOm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/suckered-tentacle.svg",
      "name": "Foul Mutation",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures are encountered, roll 1D6 Chaos Dice if of Basic Risk Factor; 2D6 Chaos Dice if of Intermediate Risk Factor; 3D6 if of Advanced Risk Factor; 4D6 if of Elite Risk Factor. For every face ‘6’, add one Taint of Chaos to the creature."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "QlJi2RfTr3jHvhO6",
      "effects": [],
      "img": "systems/zweihander/assets/icons/sword-spin.svg",
      "name": "Masterfully Adroit",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures refer to their [AB] for all Damage they inflict with weapons. They also have factored in +3 to their [AB]."
