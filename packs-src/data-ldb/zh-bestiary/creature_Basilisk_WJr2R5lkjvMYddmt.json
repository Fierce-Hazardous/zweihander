  "items": [
    {
      "_id": "6lcvatHzjEUWUJ2E",
      "effects": [],
      "img": "systems/zweihander/assets/icons/goo-spurt.svg",
      "name": "Acidic Spittle",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures can use their breath as a ranged weapon. This allows them to strike a single foe within 1+[PB], causing the foe to suffer 1D10+1 Damage from acid. However, Acidic Spittle ignores a foe’s Damage Threshold Modifier from armor. A foe can attempt to Dodge Acidic Spittle or Parry it with a shield. Acidic Spittle can be used while Engaged with foes."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "AZ9ykQqNULz7dqUm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/squid-head.svg",
      "name": "Chthonian Dweller",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures do not need to breathe and are immune to Chokehold. In addition, they can burrow or swim at the same rate of Movement as they can on foot. Finally, they may flip the results to succeed at Resolve Tests."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "3Ug7PT3QuOG0dyFn",
      "effects": [],
      "img": "systems/zweihander/assets/icons/bleeding-eye.svg",
      "name": "Eyes Wide Shut",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When this creature’s eyes are successfully struck with a Called Shot (otherwise treated as their head), they lose their ability to use Petrifying Gaze."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "ipFEoJjjBXNbONOm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/suckered-tentacle.svg",
      "name": "Foul Mutation",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures are encountered, roll 1D6 Chaos Dice if of Basic Risk Factor; 2D6 Chaos Dice if of Intermediate Risk Factor; 3D6 if of Advanced Risk Factor; 4D6 if of Elite Risk Factor. For every face ‘6’, add one Taint of Chaos to the creature."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "ztqqqP47wZEdbcMo",
      "effects": [],
      "img": "systems/zweihander/assets/icons/foam.svg",
      "name": "Gastric Acidity",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures deal Damage, a foe must Resist with Coordination. If they fail, the foe’s armor, weapon or shield is Ruined!."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "e6wmZCkNNT2VpHdH",
      "effects": [],
      "img": "systems/zweihander/assets/icons/medusa-head.svg",
      "name": "Petrifying Gaze",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When foes are Engaged with this creature (and the creature can see them), they must Resist with a successful Awareness Test or be Petrified. While Petrified, they are left Helpless and unconscious. Petrification lasts until the heart of the creature that caused the Petrification is pulverized into a bloody concoction and poured over the victim with a successful Alchemy Test."
          }
        }
      },
      "type": "trait"
    },
