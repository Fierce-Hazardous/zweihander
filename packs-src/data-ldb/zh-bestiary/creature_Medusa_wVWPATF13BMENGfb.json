    {
      "_id": "ipFEoJjjBXNbONOm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/suckered-tentacle.svg",
      "name": "Foul Mutation",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures are encountered, roll 1D6 Chaos Dice if of Basic Risk Factor; 2D6 Chaos Dice if of Intermediate Risk Factor; 3D6 if of Advanced Risk Factor; 4D6 if of Elite Risk Factor. For every face ‘6’, add one Taint of Chaos to the creature."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "SZgO4lpmgTFU05V7",
      "effects": [],
      "img": "systems/zweihander/assets/icons/surrounded-eye.svg",
      "name": "Give Me Your Eyes",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures inflict a Moderate Injury, it is always a Black Eye. When they inflict a Serious Injury, it is always Head Trauma. Finally, when they inflict a Grievous Injury, it is always a Vitreous Hemorrhage."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "cJbFNJytYMxfNPD2",
      "effects": [],
      "img": "systems/zweihander/assets/icons/axe-swing.svg",
      "name": "I Got Axe for You",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "After this creature makes an Attack Action with a melee weapon, they can immediately make an Opportunity Attack with any one-handed ranged weapon with the Throwing Quality on the same Turn."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "e6wmZCkNNT2VpHdH",
      "effects": [],
      "img": "systems/zweihander/assets/icons/medusa-head.svg",
      "name": "Petrifying Gaze",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When foes are Engaged with this creature (and the creature can see them), they must Resist with a successful Awareness Test or be Petrified. While Petrified, they are left Helpless and unconscious. Petrification lasts until the heart of the creature that caused the Petrification is pulverized into a bloody concoction and poured over the victim with a successful Alchemy Test."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "aQb57vLAyDZtXts8",
