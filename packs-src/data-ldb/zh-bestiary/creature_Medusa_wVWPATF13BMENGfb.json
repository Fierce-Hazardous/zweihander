      "type": "trait"
    },
    {
      "_id": "AZ9ykQqNULz7dqUm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/squid-head.svg",
      "name": "Chthonian Dweller",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures do not need to breathe and are immune to Chokehold. In addition, they can burrow or swim at the same rate of Movement as they can on foot. Finally, they may flip the results to succeed at Resolve Tests."
          }
        }
      },
      "type": "trait"
    },
