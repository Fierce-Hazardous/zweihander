      "effects": [],
      "img": "systems/zweihander/assets/icons/floor-hatch.svg",
      "name": "Call of the Abyss",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When this creature suffers Damage and is unable to deal Damage by the end of its next Turn, roll 3D6. If all three dice show face ‘6’, the creature is banished back to the Abyss, until summoned once again."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "oTCO3b0fIppVOz91",
      "effects": [],
      "img": "systems/zweihander/assets/icons/bandage-roll.svg",
      "name": "Bloodless",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures cannot Bleed."
          }
        }
