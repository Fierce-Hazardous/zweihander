    {
      "_id": "gafcLdG1jtuby5sI",
      "effects": [],
      "img": "systems/zweihander/assets/icons/high-five.svg",
      "name": "Both-Handedness",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When they wield 2 one-handed melee weapons and fail a Combat-based Skill Test, they may re-roll to generate a better result, but must accept the outcome."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "ipFEoJjjBXNbONOm",
      "effects": [],
      "img": "systems/zweihander/assets/icons/suckered-tentacle.svg",
      "name": "Foul Mutation",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When these creatures are encountered, roll 1D6 Chaos Dice if of Basic Risk Factor; 2D6 Chaos Dice if of Intermediate Risk Factor; 3D6 if of Advanced Risk Factor; 4D6 if of Elite Risk Factor. For every face ‘6’, add one Taint of Chaos to the creature."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "7L8NcRyzElVD8H4U",
      "effects": [],
      "img": "systems/zweihander/assets/icons/sly.svg",
      "name": "Horror of the Pit",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures do not need to breathe and are immune to Chokehold. In addition, PCs whose Order Ranks exceed their Chaos Ranks reduce their Damage by -3 when striking these creatures."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "aQb57vLAyDZtXts8",
      "effects": [],
      "img": "systems/zweihander/assets/icons/screaming.svg",
      "name": "Sanity-Blasting",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When first encountered, these creatures provoke one of the three brands of Madness; if of Basic Risk Factor, they provoke Stress; if of Intermediate Risk Factor, they provoke Fear; if of Advanced or Elite Risk Factor, they provoke Horror."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "mi9ZXkdTDkosZ2U2",
      "effects": [],
      "img": "systems/zweihander/assets/icons/brain.svg",
      "name": "Steely Fortitude",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "These creatures always succeed at Resolve Tests and cannot be Intimidated."
          }
        }
      },
      "type": "trait"
    },
    {
      "_id": "G0mUNwSnJ8ArjDll",
      "effects": [],
      "img": "systems/zweihander/assets/icons/foamy-disc.svg",
      "name": "Corrosive Bile",
      "system": {
        "description": {
          "@en": ""
        },
        "notes": "",
        "rules": {
          "effect": {
            "@en": "When creatures use this attack, it ignores their foe’s Damage Threshold Modifier from armor."
          }
        }
      },
