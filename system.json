    },
    {
      "name": "kxfin",
      "discord": "kxfin"
    }
  ],
  "media": [
    {
      "type": "cover",
      "url": "https://i.imgur.com/AsgcqBg.jpg",
      "caption": "zweihander"
    }
  ],
  "esmodules": ["index.js"],
  "styles": ["style.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "es",
      "name": "Español",
      "path": "lang/es.json"
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "lang/fr.json"
    }
  ],
  "grid": {
    "distance": 1,
    "gridUnits": "yd"
  },
