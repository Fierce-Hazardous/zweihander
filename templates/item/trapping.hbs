<div class="form-group">
  <label>Trapping Type</label>
  <input  name="data.category.value" type="text" value="{{data.category.value}}">
</div>

<div class="form-group">
  <label>Quantity</label>
  <input  name="data.quantity.value" type="number" value="{{data.quantity.value}}">
</div>

<div class="form-group">
  <label>Encumbrance (Single)</label>
